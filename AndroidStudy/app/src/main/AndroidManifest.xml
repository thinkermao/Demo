<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.hashcoding.study">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstActivity" />
        <activity android:name=".BroadcastActivity" />

        <receiver android:name=".FirstReceiver">
            <intent-filter>
                <action android:name="net.hashcoding.broadcast.static" />
            </intent-filter>
        </receiver>

        <activity android:name=".HttpConnectionActivity" />
        <activity android:name=".OKHttpActivity" />
        <activity android:name=".AsyncTaskImplActivity"></activity>
    </application>

</manifest>